{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["es6", "es2016.array.include", "es2017.object"],
    "sourceMap": true,
    "isolatedModules": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  }
}
